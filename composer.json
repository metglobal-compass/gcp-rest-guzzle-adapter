{
    "name": "metglobal/gcp-rest-guzzle-adapter",
    "description": "Simple Guzzle adapter to Google Cloud Platform REST endpoints",
    "type": "library",
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "5.3",
        "firebase/php-jwt": "4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.1"
    },
    "autoload": {
        "psr-4": {
            "GcpRestGuzzleAdapter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GcpRestGuzzleAdapter\\Tests\\": "tests/"
        }
    },
